    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:browserstack": "ember test --path ./dist --test-port=7774 --host 127.0.0.1 --config-file=browserstack.testem.js",
    "test:ember": "ember test"

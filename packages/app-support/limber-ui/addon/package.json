{
  "name": "limber-ui",
  "version": "2.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/app-support/limber-ui/addon"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "files": [
    "addon-main.cjs",
    "dist",
    "declarations"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "glint",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:hbs": "pnpm -w exec lint hbs",
    "lint:hbs:fix": "pnpm -w exec lint hbs:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "peerDependencies": {
    "@ember/test-waiters": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "ember-async-data": "^1.0.3",
    "ember-modifier": "^4.1.0",
    "ember-primitives": ">= 0.11.0",
    "ember-resources": ">= 6.4.2",
    "ember-source": "alpha",
    "webpack": ">= 5.92.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.7",
    "@embroider/addon-shim": "1.9.0",
    "@embroider/macros": "latest",
    "@fortawesome/ember-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "decorator-transforms": "^2.3.0",
    "penpal": "^6.2.2",
    "reactiveweb": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-syntax-decorators": "^7.25.7",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/preset-typescript": "^7.25.7",
    "@ember/test-waiters": "^4.0.0",
    "@embroider/addon-dev": "^7.1.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "unstable",
    "@glint/environment-ember-loose": "unstable",
    "@glint/environment-ember-template-imports": "unstable",
    "@glint/template": "unstable",
    "@nullvoxpopuli/eslint-configs": "^4.0.0",
    "@nullvoxpopuli/limber-untyped": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/ember": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.0.1",
    "ember-async-data": "1.0.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-modifier": "^4.1.0",
    "ember-primitives": "^0.27.2",
    "ember-resources": "^7.0.3",
    "ember-source": "alpha",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^6.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-ember": "^12.2.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "execa": "^9.5.2",
    "prettier": "^3.5.0",
    "prettier-plugin-ember-template-tag": "2.0.4",
    "rollup": "^4.34.6",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "~5.5.0",
    "webpack": ">= 5.92.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*"
    },
    "./theme.css": "./dist/theme.css",
    "./addon-main.js": "./addon-main.cjs"
  },
  "volta": {
    "extends": "../../../../package.json"
  }
}

{
  "name": "@nullvoxpopuli/limber-shared",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/app-support/shared"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "files": [
    "addon-main.cjs",
    "dist",
    "declarations"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "ember-tsc --noEmit",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:hbs": "pnpm -w exec lint hbs",
    "lint:hbs:fix": "pnpm -w exec lint hbs:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "peerDependencies": {
    "@ember/test-waiters": "^4.1.0",
    "@glimmer/component": "^2.0.0",
    "ember-async-data": "^2.0.0",
    "ember-modifier": "^4.1.0",
    "ember-primitives": "^0.48.0",
    "ember-resources": ">= 6.4.2",
    "ember-source": ">= 6.12.0-alpha.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@embroider/addon-shim": "1.10.2",
    "@embroider/macros": "1.19.6",
    "@fortawesome/ember-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "decorator-transforms": "2.3.1",
    "penpal": "^6.2.2",
    "reactiveweb": "1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-syntax-decorators": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@ember/library-tsconfig": "^2.0.0",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/addon-dev": "^8.0.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.1.0",
    "@glint/template": "1.7.4",
    "@glint/tsserver-plugin": "^2.1.0",
    "@nullvoxpopuli/eslint-configs": "^5.4.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@tsconfig/ember": "^3.0.9",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.2.1",
    "ember-async-data": "2.0.1",
    "ember-modifier": "^4.2.0",
    "ember-primitives": "^0.51.0",
    "ember-resources": "^7.0.7",
    "ember-source": "6.12.0-alpha.4",
    "ember-template-lint": "^7.9.3",
    "eslint": "^10.0.0",
    "execa": "^9.6.1",
    "fix-bad-declaration-output": "^1.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "2.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "shiki": "^3.20.0",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*"
    },
    "./theme.css": "./dist/theme.css",
    "./addon-main.js": "./addon-main.cjs"
  },
  "volta": {
    "extends": "../../../../package.json"
  }
}

{
  "extends": "@ember/library-tsconfig",
  "include": [
    "src/**/*",
    "unpublished-development-types/**/*"
  ],
  "exclude": ["declarations", "dist"],
  "compilerOptions": {
    "allowJs": true,
    "declarationDir": "declarations",
    "rootDir": "./src",
    "types": ["ember-source/types"]
  }
}

{
  "name": "limber-ui",
  "version": "1.0.0-beta.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/NullVoxPopuli/limber",
    "directory": "packages/limber-ui/addon"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "glint",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:hbs": "pnpm -w exec lint hbs",
    "lint:hbs:fix": "pnpm -w exec lint hbs:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-async-data": "^1.0.1",
    "ember-resources": "^6.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@ember/test-waiters": "^3.0.2",
    "@embroider/addon-shim": "1.8.5-unstable.9aaea50",
    "@embroider/macros": "1.10.1-unstable.9aaea50",
    "@fortawesome/ember-fontawesome": "^0.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "ember-modifier": "^4.1.0",
    "penpal": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-decorators": "^7.21.0",
    "@embroider/addon-dev": "3.0.1-unstable.9aaea50",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.0.2",
    "@glint/environment-ember-loose": "^1.0.2",
    "@glint/environment-ember-template-imports": "^1.0.2",
    "@glint/template": "^1.0.2",
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "@nullvoxpopuli/limber-untyped": "workspace:*",
    "@rollup/plugin-babel": "^6.0.3",
    "@tsconfig/ember": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "concurrently": "^8.0.1",
    "ember-async-data": "^1.0.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-resources": "^6.1.0",
    "ember-source": "^5.0.0",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^5.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-ember": "^11.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-ember-template-tag": "^0.3.2",
    "rollup": "^3.21.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.1",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/code.js": "./dist/_app_/components/code.js",
      "./components/external-link.js": "./dist/_app_/components/external-link.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./theme.css": "./dist/theme.css",
    "./addon-main.js": "./addon-main.cjs"
  }
}

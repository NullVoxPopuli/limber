{
  "name": "@nullvoxpopuli/horizon-theme",
  "version": "0.0.0",
  "description": "horizon variables for the limber editors and other JS-based theming",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint:types": "tsc --noEmit",
    "lint:fix": "pnpm -w exec lint fix",
    "lint": "pnpm -w exec lint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@nullvoxpopuli/eslint-configs": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "prettier": "^2.8.8",
    "prettier-plugin-ember-template-tag": "^0.3.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">= v16",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/horizon-theme"
  },
  "author": "NullVoxPopuli"
}

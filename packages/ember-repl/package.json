{
  "name": "ember-repl",
  "version": "6.0.0",
  "type": "module",
  "description": "Library for enabling REPL and Playground creation with Ember/Glimmer",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/ember-repl"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "src",
    "config"
  ],
  "scripts": {
    ":vite": "vite --config ./config/vite.config.mjs",
    ":testem": "testem --file ./config/testem.cjs",
    ":rollup": "rollup --config ./config/rollup.config.mjs",
    "build": "pnpm :rollup",
    "build:tests": "pnpm :vite build --mode=development --out-dir dist-tests",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:package": "pnpm publint",
    "lint:published-types": "attw --config-path ./config/attw.json",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "pnpm :vite dev",
    "test:ci": "pnpm :testem ci",
    "test:ember": "pnpm build:tests && pnpm test:ci",
    "prepack": "pnpm :rollup"
  },
  "dependencies": {
    "@ember/test-helpers": "^5.1.0",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/addon-shim": "^1.8.9",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "content-tag": "^3.0.0",
    "decorator-transforms": "^2.3.0",
    "ember-primitives": "^0.34.0",
    "ember-resolver": "^13.1.0",
    "ember-resources": "^7.0.3",
    "repl-sdk": "workspace:*"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.25.6",
    "@babel/standalone": "^7.28.0",
    "@ember/app-tsconfig": "^1.0.0",
    "@ember/library-tsconfig": "^1.0.0",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/addon-dev": "^8.1.0",
    "@embroider/compat": "^4.1.0",
    "@embroider/core": "^4.1.0",
    "@embroider/macros": "^1.18.0",
    "@embroider/vite": "^1.1.5",
    "@eslint/js": "^9.30.1",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.2",
    "@glint/environment-ember-loose": "^2.0.0-alpha.2",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.2",
    "@glint/template": "^1.6.0-alpha.1",
    "@glint/tsserver-plugin": "^2.0.0-alpha.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/babel__standalone": "^7.1.9",
    "@types/qunit": "^2.19.12",
    "babel-import-util": "^3.0.1",
    "babel-plugin-debug-macros": "^1.0.2",
    "babel-plugin-ember-template-compilation": "^2.2.5",
    "change-case": "^5.4.4",
    "common-tags": "^1.8.2",
    "concurrently": "^9.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "^6.3.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.21.0",
    "globals": "^16.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "publint": "^0.3.12",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rollup": "^4.44.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "testem": "^3.16.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "uuid": "^11.1.0",
    "vite": "^6.2.4",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "peerDependencies": {
    "@glint/template": ">= 1.5.2"
  },
  "peerDependenciesMeta": {
    "@glint/template": {
      "optional": true
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "config/addon-main.cjs"
  },
  "imports": {
    "#src/*": "./src/*",
    "#app/*": "./tests/app/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test-support": {
      "types": "./declarations/test-support.d.ts",
      "default": "./dist/test-support.js"
    },
    "./addon-main.js": "./config/addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  }
}

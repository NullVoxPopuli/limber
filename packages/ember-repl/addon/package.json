{
  "name": "ember-repl",
  "version": "3.0.0-beta.7",
  "description": "Addon for enabling REPL and Playground creation with Ember/Glimmer",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/ember-repl/addon"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "typesVersions": {
    "*": {
      "test-support": [
        "declarations/test-support/index.d.ts"
      ],
      "markdown/parse": [
        "./declarations/browser/compile/markdown-to-ember.d.ts"
      ],
      "*": [
        "declarations/browser/*",
        "declarations/browser/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./esm": {
      "types": "./declarations/browser/esm/index.d.ts",
      "default": "./dist/browser/esm/index.js"
    },
    "./markdown/parse": {
      "types": "./declarations/browser/compile/markdown-to-ember.d.ts",
      "default": "./dist/browser/compile/markdown-to-ember.js"
    },
    "./ember-cli": {
      "require": "./src/build/ember-cli.cjs"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./__PRIVATE__DO_NOT_USE__": {
      "types": "./declarations/browser/__PRIVATE__.d.ts",
      "default": "./dist/browser/__PRIVATE__.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "src",
    "dist",
    "declarations",
    "addon-main.cjs"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup --config",
    "build:types": "tsc --emitDeclarationOnly --noEmit false",
    "lint:types": "tsc --noEmit",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:package": "pnpm publint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:hbs": "pnpm -w exec lint hbs",
    "lint:hbs:fix": "pnpm -w exec lint hbs:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.22.5",
    "@babel/standalone": "^7.23.4",
    "@embroider/addon-shim": "1.8.7",
    "@embroider/macros": "1.13.3",
    "babel-import-util": "^2.0.1",
    "babel-plugin-ember-template-compilation": "^2.2.1",
    "broccoli-file-creator": "^2.1.1",
    "change-case": "^5.2.0",
    "common-tags": "^1.8.2",
    "content-tag": "github:NullVoxPopuli/content-tag#browser-support-dist",
    "line-column": "^1.0.2",
    "magic-string": "^0.30.5",
    "mdast": "^3.0.0",
    "parse-static-imports": "^1.1.0",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.4",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0",
    "uuid": "^9.0.1",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/types": "^7.23.4",
    "@ember/test-helpers": "^3.2.1",
    "@ember/test-waiters": "^3.1.0",
    "@embroider/addon-dev": "4.1.2",
    "@glimmer/compiler": "^0.85.13",
    "@glimmer/component": "^1.1.2",
    "@glimmer/interfaces": "^0.85.13",
    "@glimmer/reference": "^0.85.13",
    "@glimmer/syntax": "^0.85.13",
    "@glimmer/tracking": "^1.1.2",
    "@glimmer/util": "^0.85.13",
    "@glint/core": "^1.2.1",
    "@glint/environment-ember-loose": "^1.2.1",
    "@glint/environment-ember-template-imports": "^1.2.1",
    "@glint/template": "^1.2.1",
    "@nullvoxpopuli/eslint-configs": "^3.2.2",
    "@nullvoxpopuli/limber-untyped": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@tsconfig/ember": "^3.0.2",
    "@types/babel__core": "^7.20.4",
    "@types/babel__standalone": "^7.1.7",
    "@types/babel__traverse": "^7.20.4",
    "@types/hast": "^3.0.3",
    "@types/mdast": "^4.0.3",
    "@types/unist": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "ember-resources": "^6.4.2",
    "ember-source": "^5.4.0",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^5.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-ember-template-tag": "^1.1.0",
    "publint": "^0.2.5",
    "rollup": "~4.6.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.1",
    "typescript": "^5.2.2"
  },
  "volta": {
    "extends": "../../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "peerDependencies": {
    "@glimmer/compiler": "^0.85.13",
    "@glimmer/component": "^1.1.2",
    "@glimmer/syntax": "^0.85.13",
    "@glint/template": "^1.2.1",
    "ember-resources": "^6.4.2",
    "ember-source": "^5.4.0"
  },
  "engines": {
    "node": ">= v16",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  }
}

{
  "name": "ember-repl",
  "version": "3.0.0-beta.0",
  "description": "Addon for enabling REPL and Playground creation with Ember/Glimmer",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/NullVoxPopuli/limber",
    "directory": "packages/ember-repl/addon"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "typesVersions": {
    "*": {
      "*": [
        "dist/browser/*",
        "dist/browser/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/browser/index.js",
    "./esm": "./dist/browser/esm/index.js",
    "./ember-cli": {
      "require": "./src/build/ember-cli.cjs"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "src",
    "dist",
    "addon-main.cjs"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "glint",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:hbs": "pnpm -w exec lint hbs",
    "lint:hbs:fix": "pnpm -w exec lint hbs:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.21.5",
    "@babel/standalone": "^7.21.8",
    "@embroider/addon-shim": "1.8.5-unstable.9aaea50",
    "@embroider/macros": "1.10.1-unstable.9aaea50",
    "babel-import-util": "^1.3.0",
    "babel-plugin-ember-template-compilation": "^2.0.2",
    "broccoli-file-creator": "^2.1.1",
    "change-case": "^4.1.2",
    "common-tags": "^1.8.2",
    "line-column": "^1.0.2",
    "magic-string": "^0.30.0",
    "parse-static-imports": "^1.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/types": "^7.21.5",
    "@ember/test-helpers": "^2.9.3",
    "@ember/test-waiters": "^3.0.2",
    "@embroider/addon-dev": "3.0.1-unstable.9aaea50",
    "@glimmer/compiler": "^0.84.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/interfaces": "^0.84.3",
    "@glimmer/reference": "^0.84.3",
    "@glimmer/syntax": "^0.84.3",
    "@glimmer/tracking": "^1.1.2",
    "@glimmer/util": "^0.84.3",
    "@glint/core": "^1.0.2",
    "@glint/environment-ember-loose": "^1.0.2",
    "@glint/environment-ember-template-imports": "^1.0.2",
    "@glint/template": "^1.0.2",
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "@nullvoxpopuli/limber-untyped": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@tsconfig/ember": "^2.0.0",
    "@types/babel__core": "^7.20.0",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__traverse": "^7.18.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "concurrently": "^8.0.1",
    "ember-source": "^5.0.0",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^5.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-ember": "^11.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-ember-template-tag": "^0.3.2",
    "rollup": "^3.21.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.1",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.0.4"
  },
  "volta": {
    "extends": "../../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "peerDependencies": {
    "@glimmer/compiler": "^0.84.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/syntax": "^0.84.3",
    "ember-source": "^5.0.0"
  }
}

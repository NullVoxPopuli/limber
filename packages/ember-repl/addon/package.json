{
  "name": "ember-repl",
  "version": "6.0.0",
  "description": "Addon for enabling REPL and Playground creation with Ember/Glimmer",
  "type": "module",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/NullVoxPopuli/limber.git",
    "directory": "packages/ember-repl/addon"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "typesVersions": {
    "*": {
      "test-support": [
        "declarations/test-support/index.d.ts"
      ],
      "markdown/parse": [
        "./declarations/compile/markdown-to-ember.d.ts"
      ],
      "*": [
        "declarations/*",
        "declarations/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./__PRIVATE__DO_NOT_USE__": {
      "types": "./declarations/__PRIVATE__.d.ts",
      "default": "./dist/__PRIVATE__.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "src",
    "dist",
    "declarations",
    "addon-main.cjs"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "tsc --noEmit",
    "lint:fix": "pnpm -w exec lint fix",
    "start": "rollup --config --watch",
    "lint": "pnpm -w exec lint",
    "lint:package": "pnpm publint",
    "lint:js": "pnpm -w exec lint js",
    "lint:js:fix": "pnpm -w exec lint js:fix",
    "lint:prettier:fix": "pnpm -w exec lint prettier:fix",
    "lint:prettier": "pnpm -w exec lint prettier"
  },
  "dependencies": {
    "@ember/test-helpers": "^5.1.0",
    "@embroider/addon-shim": "1.10.0",
    "@embroider/macros": "1.18.0",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "broccoli-file-creator": "^2.1.1",
    "content-tag": "^3.0.0",
    "decorator-transforms": "^2.3.0",
    "ember-resolver": "^13.1.0",
    "ember-resources": "^7.0.3",
    "repl-sdk": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/helper-plugin-utils": "^7.26.5",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/standalone": "^7.26.9",
    "@babel/types": "^7.26.9",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/addon-dev": "^8.0.1",
    "@glimmer/compiler": "^0.94.10",
    "@glimmer/component": "^2.0.0",
    "@glimmer/interfaces": "^0.94.5",
    "@glimmer/reference": "^0.94.8",
    "@glimmer/syntax": "^0.94.9",
    "@glimmer/tracking": "^1.1.2",
    "@glimmer/util": "^0.94.6",
    "@glint/core": "alpha",
    "@glint/environment-ember-loose": "alpha",
    "@glint/environment-ember-template-imports": "alpha",
    "@glint/template": "1.5.2",
    "@nullvoxpopuli/eslint-configs": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@tsconfig/ember": "^3.0.9",
    "@types/babel__core": "^7.20.5",
    "@types/babel__standalone": "^7.1.9",
    "@types/babel__traverse": "^7.20.6",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "babel-import-util": "^3.0.0",
    "babel-plugin-debug-macros": "^1.0.2",
    "change-case": "^5.4.4",
    "common-tags": "^1.8.2",
    "concurrently": "^9.1.2",
    "ember-resources": "^7.0.0",
    "ember-source": ">= 6.7.0-alpha.1",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.1",
    "eslint": "^9.29.0",
    "execa": "^9.5.2",
    "line-column": "^1.0.2",
    "magic-string": "^0.30.17",
    "mdast": "^3.0.0",
    "parse-static-imports": "^1.1.0",
    "prettier": "^3.5.2",
    "prettier-plugin-ember-template-tag": "2.0.6",
    "publint": "^0.2.12",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rollup": "~4.44.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "typescript": "5.8.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "uuid": "^11.1.0",
    "vfile": "^6.0.3",
    "webpack": "^5.98.0"
  },
  "volta": {
    "extends": "../../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "externals": [
      "ember-template-compiler"
    ],
    "app-js": {}
  },
  "peerDependencies": {
    "@glimmer/compiler": ">= 0.86.0",
    "@glimmer/syntax": ">= 0.86.0",
    "@glint/template": "1.5.2",
    "webpack": ">= 5.92.0"
  },
  "peerDependenciesMeta": {
    "@glint/template": {
      "optional": true
    },
    "@glimmer/compiler": {
      "optional": true
    },
    "@glimmer/syntax": {
      "optional": true
    }
  }
}

    "@babel/runtime": "^7.26.0",
    "@babel/standalone": "^7.26.2",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/compat": "3.7.1-unstable.0aa8fc6",

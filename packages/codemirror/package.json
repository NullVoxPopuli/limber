{
  "name": "@nullvoxpopuli/limber-codemirror",
  "version": "0.0.0",
  "description": "custom build of codemirror for use in Limber",
  "main": "index.js",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "dev": "echo 'limber-codemirror does not have a watch mode'; yarn build",
    "check:types": "tsc --build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.19.12",
    "@codemirror/basic-setup": "0.19.1",
    "@codemirror/closebrackets": "0.19.0",
    "@codemirror/commands": "0.19.8",
    "@codemirror/comment": "0.19.0",
    "@codemirror/highlight": "0.19.7",
    "@codemirror/history": "0.19.2",
    "@codemirror/lang-javascript": "0.19.7",
    "@codemirror/lang-markdown": "0.19.6",
    "@codemirror/search": "0.19.7",
    "@codemirror/state": "0.19.7",
    "@codemirror/text": "0.19.6",
    "@codemirror/view": "0.19.42",
    "@nullvoxpopuli/horizon-theme": "*",
    "broccoli-funnel": "^3.0.8"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "2.1.21",
    "esbuild": "0.14.23",
    "esbuild-plugin-alias": "^0.2.1",
    "recursive-copy": "^2.0.14"
  },
  "engines": {
    "node": ">= v16.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}

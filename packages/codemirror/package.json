{
  "name": "@nullvoxpopuli/limber-codemirror",
  "version": "0.0.0",
  "description": "custom build of codemirror for use in Limber",
  "main": "index.js",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "check:types": "tsc --build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.19.9",
    "@codemirror/basic-setup": "0.19.0",
    "@codemirror/closebrackets": "0.19.0",
    "@codemirror/commands": "0.19.5",
    "@codemirror/comment": "0.19.0",
    "@codemirror/highlight": "0.19.6",
    "@codemirror/history": "0.19.0",
    "@codemirror/lang-javascript": "0.19.3",
    "@codemirror/lang-markdown": "0.19.2",
    "@codemirror/search": "0.19.3",
    "@codemirror/state": "0.19.6",
    "@codemirror/text": "0.19.5",
    "@codemirror/view": "0.19.24",
    "@nullvoxpopuli/horizon-theme": "*",
    "broccoli-funnel": "^3.0.8"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "1.4.10",
    "esbuild": "0.13.13",
    "esbuild-plugin-alias": "^0.2.0",
    "recursive-copy": "^2.0.13"
  },
  "engines": {
    "node": ">= v16.13.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}

{
  "name": "limber-snippet",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint:types": "glint",
    "lint:prettier:fix": "prettier -w .",
    "lint:prettier": "prettier -c .",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-async-data": "^0.7.0",
    "ember-resources": "^6.0.0-beta.0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-decorators": "^7.21.0",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.0.0-beta.3",
    "@glint/environment-ember-loose": "^1.0.0-beta.3",
    "@glint/environment-ember-template-imports": "^1.0.0-beta.3",
    "@glint/template": "^1.0.0-beta.3",
    "@nullvoxpopuli/eslint-configs": "^3.1.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@tsconfig/ember": "^2.0.0",
    "concurrently": "^7.6.0",
    "ember-async-data": "^0.7.0",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-resources": "^6.0.0-beta.1",
    "ember-source": "^4.11.0",
    "ember-template-imports": "^3.4.1",
    "ember-template-lint": "^4.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^11.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rollup": "^3.19.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/code/index.js": "./dist/_app_/components/code/index.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}

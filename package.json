{
  "name": "limber-monorepo",
  "private": true,
  "author": "NullVoxPopuli",
  "workspaces": [
    "frontend",
    "packages/*",
    "tests"
  ],
  "resolutions": {
    "tracked-built-ins": "github:NullVoxPopuli/tracked-built-ins#tracked-storage",
    "tracked-maps-and-sets": "3.0.2"
  },
  "scripts": {
    "predeploy": "yarn build && yarn workspace limber build",
    "build": "yarn nx run-many --target=build --all",
    "lint:fix": "yarn nx run-many --target=lint:js --all",
    "start": "yarn nx run limber:start"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17",
    "npm": "8.1.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@nrwl/workspace": "latest",
    "@nrwl/cli": "latest",
    "@nrwl/tao": "latest",
    "typescript": "4.2.4"
  }
}
